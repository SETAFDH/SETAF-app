                      <locus type="page">page de titre</locus>
                      <objectType>Gravure sur bois</objectType>
                      <bibl source="https://mdz-nbn-resolving.de/details:bsb00088818">
                        <title type="titre_ouvrage">Martin Luther, Wider das Bapstum zu Rom</title>
                        <author role="artiste">Atelier de Lucas Cranach</author>
                        <publisher>Hans Lufft</publisher>
                        <pubPlace>Wittenberg</pubPlace>
