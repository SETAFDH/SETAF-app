                      <locus type="page">page de titre</locus>
                      <objectType>Gravure sur bois</objectType>
                      <bibl source="https://doi.org/10.3931/e-rara-1726 ">
                        <title type="titre_ouvrage">Ulrich Zwingli, Vonn dem Nachtmal Christi</title>
                        <author role="artiste"/>
                        <publisher>Christoph Froschauer</publisher>
                        <pubPlace>Zurich</pubPlace>
                        <date>1525</date>
                        <idno type="VD16" corresp="https://gateway-bayern.de/VD16+Z+917">VD16 n° Z 917</idno>
                        <idno type="USTC" corresp="https://www.ustc.ac.uk/editions/704219">USTC n° 704219</idno>
